{
  "name": "rails-scratch",
	"displayName": "rails-scratch",
	"description": "Run arbritary rails code",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.62.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
        "onCommand:rails-scratch.helloWorld",
				"onCommand:rails-scratch.runFile"

	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "rails-scratch.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "rails-scratch.runFile",
				"title": "Run rails file"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.62.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.1.0",
		"@typescript-eslint/parser": "^5.1.0",
		"eslint": "^8.1.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.3",
		"typescript": "^4.4.4",
		"@vscode/test-electron": "^1.6.2"
	}
}
